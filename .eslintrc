{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest/globals": true
  },
  "plugins": [ "jest" ],
  "rules": {
    "semi": "off",
    "keyword-spacing": "off",
    "linebreak-style": "off",
    "require-jsdoc": "off",
    "arrow-parens": "off",
    "comma-dangle": "off"
  },
  "extends": [
    "eslint:recommended", "google"
  ]
}